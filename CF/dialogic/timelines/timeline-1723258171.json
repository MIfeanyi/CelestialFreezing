		{
			"character": "character-1701671588.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": " Yada yada yada. "
		},
		{
			"character": "character-1701671588.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "You know, just basic survival stuff, nothing crazy."
