		},
		{
			"character": "character-1701671588.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "Alright, I’m counting on you."
		},
		{
			"character": "character-1701671588.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "Let’s make the world a better place!"
		},
		{
			"character": "character-1701672712.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "Lowen fist bumps you."
		},
		{
			"character": "character-1701672712.json",
			"event_id": "dialogic_001",
			"portrait": "",
			"text": "Lowen is now your partner!"
